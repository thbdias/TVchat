<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" styleClass="mainFxmlClass" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.131" fx:controller="tvchat.Telas.Chat.ChatController">
    <stylesheets>
        <URL value="@chat.css" />
    </stylesheets>
   <children>
      <SplitPane dividerPositions="0.5">
         <items>
            <ListView id="list_clients" />
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <ListView id="list_chat" />
                  <TextArea id="text_message" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </VBox>
         </items>
      </SplitPane>
   </children>
</AnchorPane>
